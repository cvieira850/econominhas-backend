	"scripts": {
		"docs:db": "dbdocs build ./database.dbml",
		"docs:serve": "redocly preview-docs",
		"docs:build": "redocly bundle -o dist/bundle.yaml",
		"docs:lint": "redocly lint",
		"prepare": "husky install",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:docker": "nest start --watch",
		"start:dev": "env-cmd nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"src/**/*.ts\" --fix --quiet",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"husky:pre-commit": "tsc && yarn lint"
	}
